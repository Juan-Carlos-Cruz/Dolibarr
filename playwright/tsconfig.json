{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "types": ["node", "@playwright/test"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": false,
    "allowJs": true,
    "outDir": "../dist"
  },
  "include": ["./**/*.ts"],
  "exclude": ["../node_modules"]
}
